{"ast":null,"code":"import HeaderPage from \"../components/layout/Header.vue\";\nimport FooterPage from \"../components/layout/Footer.vue\";\nimport TodoItems from \"../components/Todos.vue\";\nimport AddTodo from \"../components/AddTodo.vue\";\nexport default {\n  name: 'HomeView',\n  components: {\n    AddTodo,\n    HeaderPage,\n    FooterPage,\n    TodoItems\n  },\n  data() {\n    return {\n      componentKey: 0,\n      todos: [],\n      username: \"\"\n    };\n  },\n  methods: {\n    forceRerender() {\n      this.componentKey += 1;\n    },\n    deleteItem(id) {\n      this.todos = this.todos.filter(todo => todo.id != id);\n      this.$store.dispatch(\"deleteTodo\", id);\n    },\n    addItem(newTodo) {\n      this.todos = [...this.todos, newTodo];\n      this.$store.dispatch(\"addToTodos\", newTodo);\n    },\n    editTodo(editedTodo) {\n      let i = 0;\n      while (i < this.todos.length) {\n        if (this.todos[i].id == editedTodo.id) {\n          console.log(\"Edition completed!\");\n          this.todos[i] = editedTodo;\n        }\n        i++;\n      }\n      this.forceRerender();\n      this.$store.dispatch(\"editTodo\", editedTodo);\n    }\n  },\n  mounted() {\n    this.todos = this.$store.state.todos;\n    console.log(this.username);\n    this.username = this.$store.state.username;\n  }\n};","map":{"version":3,"names":["HeaderPage","FooterPage","TodoItems","AddTodo","name","components","data","componentKey","todos","username","methods","forceRerender","deleteItem","id","filter","todo","$store","dispatch","addItem","newTodo","editTodo","editedTodo","i","length","console","log","mounted","state"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <HeaderPage/>\n    <h1>{{ username }}</h1>\n    <AddTodo v-on:add-todo=\"addItem\"/>\n    <TodoItems :key=\"componentKey\" v-bind:todos=\"todos\"  v-on:delete-todo=\"deleteItem\"  v-on:edition=\"editTodo\"/>\n    <FooterPage/>\n  </div>\n</template>\n\n<script>\nimport HeaderPage from \"../components/layout/Header.vue\";\nimport FooterPage from \"../components/layout/Footer.vue\";\nimport TodoItems from \"../components/Todos.vue\";\nimport AddTodo from \"../components/AddTodo.vue\";\n\n\nexport default {\n  name: 'HomeView',\n  components: {\n    AddTodo,\n    HeaderPage,\n    FooterPage,\n    TodoItems\n  },\n  data () {\n    return {\n      componentKey: 0,\n      todos: [],\n      username: \"\",\n    }\n  },\n  methods: {\n    forceRerender() {\n      this.componentKey += 1;\n    },\n    deleteItem (id) {\n      this.todos = this.todos.filter( todo => todo.id != id);\n      this.$store.dispatch(\"deleteTodo\", id);\n    },\n    addItem (newTodo) {\n      this.todos = [...this.todos, newTodo];\n      this.$store.dispatch(\"addToTodos\", newTodo);\n    },\n    editTodo (editedTodo) {\n      let i = 0; \n      while (i < this.todos.length) {\n        if (this.todos[i].id == editedTodo.id) {\n          console.log(\"Edition completed!\");\n          this.todos[i] = editedTodo;\n        }\n        i ++;\n      }\n      this.forceRerender();\n      this.$store.dispatch(\"editTodo\", editedTodo);\n    },\n  },\n  mounted () {\n    this.todos = this.$store.state.todos;\n    console.log(this.username);\n    this.username = this.$store.state.username;\n    \n\n  },\n}\n</script>\n"],"mappings":"AAWA,OAAAA,UAAA;AACA,OAAAC,UAAA;AACA,OAAAC,SAAA;AACA,OAAAC,OAAA;AAGA;EACAC,IAAA;EACAC,UAAA;IACAF,OAAA;IACAH,UAAA;IACAC,UAAA;IACAC;EACA;EACAI,KAAA;IACA;MACAC,YAAA;MACAC,KAAA;MACAC,QAAA;IACA;EACA;EACAC,OAAA;IACAC,cAAA;MACA,KAAAJ,YAAA;IACA;IACAK,WAAAC,EAAA;MACA,KAAAL,KAAA,QAAAA,KAAA,CAAAM,MAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAF,EAAA,IAAAA,EAAA;MACA,KAAAG,MAAA,CAAAC,QAAA,eAAAJ,EAAA;IACA;IACAK,QAAAC,OAAA;MACA,KAAAX,KAAA,YAAAA,KAAA,EAAAW,OAAA;MACA,KAAAH,MAAA,CAAAC,QAAA,eAAAE,OAAA;IACA;IACAC,SAAAC,UAAA;MACA,IAAAC,CAAA;MACA,OAAAA,CAAA,QAAAd,KAAA,CAAAe,MAAA;QACA,SAAAf,KAAA,CAAAc,CAAA,EAAAT,EAAA,IAAAQ,UAAA,CAAAR,EAAA;UACAW,OAAA,CAAAC,GAAA;UACA,KAAAjB,KAAA,CAAAc,CAAA,IAAAD,UAAA;QACA;QACAC,CAAA;MACA;MACA,KAAAX,aAAA;MACA,KAAAK,MAAA,CAAAC,QAAA,aAAAI,UAAA;IACA;EACA;EACAK,QAAA;IACA,KAAAlB,KAAA,QAAAQ,MAAA,CAAAW,KAAA,CAAAnB,KAAA;IACAgB,OAAA,CAAAC,GAAA,MAAAhB,QAAA;IACA,KAAAA,QAAA,QAAAO,MAAA,CAAAW,KAAA,CAAAlB,QAAA;EAGA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}