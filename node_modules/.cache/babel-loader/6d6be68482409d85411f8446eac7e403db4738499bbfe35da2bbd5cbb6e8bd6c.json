{"ast":null,"code":"//import HeaderPage from \"../components/layout/Header.vue\";\n//import FooterPage from \"../components/layout/Footer.vue\";\n//import TodoItems from \"../components/Todos.vue\";\nimport AddTodo from \"../components/AddTodo.vue\";\nimport TodoList from \"../components/vuetify/TodoList.vue\";\nexport default {\n  name: 'HomeView',\n  components: {\n    AddTodo,\n    //TodoItems,\n    TodoList\n  },\n  data() {\n    return {\n      TodosKey: 0,\n      UserKey: 1000,\n      todos: [],\n      username: \"User Undefined\"\n    };\n  },\n  methods: {\n    forceRerender() {\n      this.TodosKey += 1;\n    },\n    forceRerender1() {\n      this.UserKey += 1;\n    },\n    deleteItem(id) {\n      this.todos = this.todos.filter(todo => todo.id != id);\n      this.$store.dispatch(\"deleteTodo\", id);\n    },\n    addItem(newTodo) {\n      this.todos = [...this.todos, newTodo];\n      this.$store.dispatch(\"addToTodos\", newTodo);\n    },\n    editTodo(editedTodo) {\n      let i = 0;\n      while (i < this.todos.length) {\n        if (this.todos[i].id == editedTodo.id) {\n          console.log(\"Edition completed!\");\n          this.todos[i] = editedTodo;\n        }\n        i++;\n      }\n      this.forceRerender();\n      this.$store.dispatch(\"editTodo\", editedTodo);\n    }\n  },\n  mounted() {\n    this.username = this.$store.state.user.username;\n    this.todos = this.$store.state.todos;\n  }\n};","map":{"version":3,"names":["AddTodo","TodoList","name","components","data","TodosKey","UserKey","todos","username","methods","forceRerender","forceRerender1","deleteItem","id","filter","todo","$store","dispatch","addItem","newTodo","editTodo","editedTodo","i","length","console","log","mounted","state","user"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home pa-6\">\n    <!-- <HeaderPage/> -->\n    <h2 class=\"text-center text-h4\">Todo-List</h2>\n    <div class=\"username\">\n      <h2 class=\"text-h5\">Welcome {{ username }}</h2>\n    </div>\n    <!-- <h2 :key=\"UserKey\">Welcome {{ username }}</h2> -->\n    <div class=\"addTodo\">\n      <AddTodo v-on:add-todo=\"addItem\"/>\n    </div>\n    \n    <!-- <TodoItems :key=\"TodosKey\" v-bind:todos=\"todos\" v-bind:archive=\"false\" v-on:delete-todo=\"deleteItem\"  v-on:edition=\"editTodo\"/> -->\n    <TodoList\n      :key=\"TodosKey\" \n      v-bind:todos=\"todos\" \n      v-bind:archive=\"false\" \n      v-on:delete-todo=\"deleteItem\"  \n      v-on:edition=\"editTodo\"\n    />\n    <!-- <FooterPage/> -->\n  </div>\n</template>\n\n<script>\n//import HeaderPage from \"../components/layout/Header.vue\";\n//import FooterPage from \"../components/layout/Footer.vue\";\n//import TodoItems from \"../components/Todos.vue\";\nimport AddTodo from \"../components/AddTodo.vue\";\nimport TodoList from \"../components/vuetify/TodoList.vue\";\n\nexport default {\n  name: 'HomeView',\n  components: {\n    AddTodo,\n    //TodoItems,\n    TodoList,\n  },\n  data () {\n    return {\n      TodosKey: 0,\n      UserKey: 1000,\n      todos: [],\n      username: \"User Undefined\",\n    }\n  },\n  methods: {\n    forceRerender() {\n      this.TodosKey += 1;\n    },\n    forceRerender1() {\n      this.UserKey += 1;\n    },\n    deleteItem (id) {\n      this.todos = this.todos.filter( todo => todo.id != id);\n      this.$store.dispatch(\"deleteTodo\", id);\n    },\n    addItem (newTodo) {\n      this.todos = [...this.todos, newTodo];\n      this.$store.dispatch(\"addToTodos\", newTodo);\n    },\n    editTodo (editedTodo) {\n      let i = 0; \n      while (i < this.todos.length) {\n        if (this.todos[i].id == editedTodo.id) {\n          console.log(\"Edition completed!\");\n          this.todos[i] = editedTodo;\n        }\n        i ++;\n      }\n      this.forceRerender();\n      this.$store.dispatch(\"editTodo\", editedTodo);\n    },\n  },\n  mounted () {\n    this.username = this.$store.state.user.username;\n    this.todos = this.$store.state.todos;\n  },\n}\n</script>\n\n<style>\n  .addTodo {\n    width: 350px;\n  }\n</style>\n"],"mappings":"AAyBA;AACA;AACA;AACA,OAAAA,OAAA;AACA,OAAAC,QAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAH,OAAA;IACA;IACAC;EACA;EACAG,KAAA;IACA;MACAC,QAAA;MACAC,OAAA;MACAC,KAAA;MACAC,QAAA;IACA;EACA;EACAC,OAAA;IACAC,cAAA;MACA,KAAAL,QAAA;IACA;IACAM,eAAA;MACA,KAAAL,OAAA;IACA;IACAM,WAAAC,EAAA;MACA,KAAAN,KAAA,QAAAA,KAAA,CAAAO,MAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAF,EAAA,IAAAA,EAAA;MACA,KAAAG,MAAA,CAAAC,QAAA,eAAAJ,EAAA;IACA;IACAK,QAAAC,OAAA;MACA,KAAAZ,KAAA,YAAAA,KAAA,EAAAY,OAAA;MACA,KAAAH,MAAA,CAAAC,QAAA,eAAAE,OAAA;IACA;IACAC,SAAAC,UAAA;MACA,IAAAC,CAAA;MACA,OAAAA,CAAA,QAAAf,KAAA,CAAAgB,MAAA;QACA,SAAAhB,KAAA,CAAAe,CAAA,EAAAT,EAAA,IAAAQ,UAAA,CAAAR,EAAA;UACAW,OAAA,CAAAC,GAAA;UACA,KAAAlB,KAAA,CAAAe,CAAA,IAAAD,UAAA;QACA;QACAC,CAAA;MACA;MACA,KAAAZ,aAAA;MACA,KAAAM,MAAA,CAAAC,QAAA,aAAAI,UAAA;IACA;EACA;EACAK,QAAA;IACA,KAAAlB,QAAA,QAAAQ,MAAA,CAAAW,KAAA,CAAAC,IAAA,CAAApB,QAAA;IACA,KAAAD,KAAA,QAAAS,MAAA,CAAAW,KAAA,CAAApB,KAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}