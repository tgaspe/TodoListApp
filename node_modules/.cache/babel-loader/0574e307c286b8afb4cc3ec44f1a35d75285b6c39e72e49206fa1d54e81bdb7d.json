{"ast":null,"code":"import TodoItem from \"./TodoItem.vue\";\nexport default {\n  name: \"TodoItems\",\n  components: {\n    TodoItem\n  },\n  props: {\n    archive: {\n      type: Boolean,\n      required: true\n    },\n    todos: {\n      type: Array,\n      required: true\n    }\n  },\n  methods: {\n    submitEditions(e) {\n      this.$emit(\"edition\", e);\n    },\n    archiveTodos(e) {\n      console.log(\"received in todos\");\n      console.log(e);\n      this.$store.dispatch(\"addToArchive\", e);\n      this.$emit('delete-todo', e.id);\n    }\n  }\n};","map":{"version":3,"names":["TodoItem","name","components","props","archive","type","Boolean","required","todos","Array","methods","submitEditions","e","$emit","archiveTodos","console","log","$store","dispatch","id"],"sources":["src/components/Todos.vue"],"sourcesContent":["<template>\n    <div>\n        <div v-for=\"todo in todos\" v-bind:key=\"todo.id\" >\n            <TodoItem v-bind:todo=\"todo\"  v-on:update:completed=\"todo.completed = $event\" v-on:delete-item=\"$emit('delete-todo', todo.id)\" v-on:edition=\"submitEditions\" v-on:archiveTodo=\"archiveTodos\" v-bind:inArchive=\"archive\"/>\n        </div>\n    </div>\n</template>\n\n<script>\nimport TodoItem from \"./TodoItem.vue\";\n\nexport default {\n    name: \"TodoItems\",\n    components: {\n        TodoItem\n    },\n    props: {\n        archive: {\n            type: Boolean,\n            required: true,\n        },\n        todos: {\n            type: Array,\n            required: true,\n        }\n    },\n    methods: {\n        submitEditions (e) {\n            this.$emit(\"edition\", e);\n        },\n        archiveTodos (e) {\n            console.log(\"received in todos\")\n            console.log(e);\n            this.$store.dispatch(\"addToArchive\", e);\n            this.$emit('delete-todo', e.id);\n        }\n\n    }\n}\n\n</script>\n\n<style scoped>\n\n</style>"],"mappings":"AASA,OAAAA,QAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAG,KAA